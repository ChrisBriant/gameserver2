[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=django
Group=www-data
WorkingDirectory=/home/chris/Documents/django/gameserver2
ExecStart=/home/chris/Documents/django/environments/gameserver/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/chris/Documents/django/gameserver2/gameserver.sock gameserver.wsgi:application

[Install]
WantedBy=multi-user.target
