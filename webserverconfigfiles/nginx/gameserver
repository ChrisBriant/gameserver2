server {
    listen 80;
    server_name 192.168.56.101 gameserver;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/chris/Documents/django/gameserver2/templates;
    }

    location /ws/ {
	proxy_pass http://unix:/home/chris/Documents/django/gameserver2/daphne.sock;
    }


    location / {
        include proxy_params;
        proxy_pass http://unix:/home/chris/Documents/django/gameserver2/gameserver.sock;
    }
}
